{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "GithubIntegration": {
    "BaseUri": "https://localhost:5001/api/",
    "RepositoriesByUserUri": "GitRepository/GetByUser/{0}",
    "RepositoriesByNameUri": "GitRepository/GetByName/{0}",
    "RepositoryByOwnerAndRepository": "GitRepository/GetByOwnerAndRepository?nameRepository={0}&loginOwner={1}",
    "RepositoryContributors": "GitRepository/GetContributorsByRepository?nameRepository={0}&loginOwner={1}",
    "RepositoryFavoritePath": "C:\\Users\\Public\\Documents\\GitIntegration\\"
  }
}